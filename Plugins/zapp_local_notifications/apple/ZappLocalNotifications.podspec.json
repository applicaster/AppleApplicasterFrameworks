{
  "name": "ZappLocalNotifications",
  "version": "0.1.0",
  "platforms": {
    "ios": "11.0",
    "tvos": "11.0"
  },
  "summary": "ZappLocalNotifications",
  "description": "ZappLocalNotifications container.",
  "homepage": "https://github.com/applicaster/AppleApplicasterFrameworks.git",
  "license": "CMPS",
  "authors": "Applicaster LTD.",
  "source": {
    "git": "https://github.com/applicaster/AppleApplicasterFrameworks.git",
    "branch": "master"
  },
  "requires_arc": true,
  "static_framework": true,
  "swift_versions": "5.1",
  "source_files": "Universal/**/*.swift",
