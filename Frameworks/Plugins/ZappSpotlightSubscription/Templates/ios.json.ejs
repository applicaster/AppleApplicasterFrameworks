{
  "api": {
    "require_startup_execution": true,
    "class_name": "ZPSpotlightSubscription",
    "modules": [
      "ZappSpotlightSubscription"
    ],
    "plist": {
      "UISupportsTVApp": true
    },
    "entitlements": {
      "com.apple.smoot.subscriptionservice": true
    }
  },
  "npm_dependencies": [
    "@applicaster/spotlight-subscription-apple@<%= version_id %>"
  ],
  "extra_dependencies": [
    {
      "ZappSpotlightSubscription": ":path => './node_modules/@applicaster/spotlight-subscription-apple/ZappSpotlightSubscription.podspec'"
    }
  ],
  "platform": "ios",
  "author_name": "Alex Zchut",
  "author_email": "a.zchut@applicaster.com",
  "manifest_version": "<%= version_id %>",
  "name": "Spotlight Subscription",
  "description": "Spotlight Subscription",
  "type": "general",
  "screen": false,
  "identifier": "SpotlightSubscription",
  "ui_builder_support": true,
  "whitelisted_account_ids": [],
  "min_zapp_sdk": "20.0.0-Dev",
  "deprecated_since_zapp_sdk": "",
  "unsupported_since_zapp_sdk": "",
  "react_native": false,
  "custom_configuration_fields": [
    {
      "type": "checkbox",
      "key": "enabled",
      "tooltip_text": "Is plugin enabled on app start",
      "default": 0
    }
  ],
  "targets": [
    "mobile"
  ]
}
