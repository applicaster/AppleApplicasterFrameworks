{
  "name": "ZappGoogleInteractiveMediaAds",
  "version": "0.7.0",
  "swift_versions": "5.1",
  "summary": "ZappGoogleInteractiveMediaAds",
  "description": "This plugin allow to add Google Interactive Media Ads to supported players.",
  "homepage": "https://github.com/applicaster/AppleApplicasterFrameworks.git",
  "license": "Appache 2.0",
  "authors": "Applicaster LTD.",
  "source": {
    "git": "https://github.com/applicaster/AppleApplicasterFrameworks.git",
    "branch": "master"
  },
  "platforms": {
    "ios": "10.0",
    "tvos": "10.0"
  },
  "tvos": {
    "vendored_frameworks": "tvOS/GoogleInteractiveMediaAds.framework",
    "preserve_paths": "tvOS/GoogleInteractiveMediaAds.framework",
    "source_files": [
      "Universal/**/*.swift",
      "tvOS/**/*.swift"
    ]
  },
  "ios": {
    "source_files": "Universal/**/*.swift",
    "dependencies": {
      "GoogleAds-IMA-iOS-SDK": [
        "= 3.11.1"
      ]
    }
  },
  "dependencies": {
    "ZappCore": [

    ]
  },
  "xcconfig": {
    "ENABLE_BITCODE": "YES",
    "OTHER_LDFLAGS": "$(inherited)  -framework \"GoogleInteractiveMediaAds\"",
    "LIBRARY_SEARCH_PATHS": "$(inherited) \"${PODS_ROOT}\"/**",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "ENABLE_TESTABILITY": "YES",
    "OTHER_CFLAGS": "-fembed-bitcode",
    "FRAMEWORK_SEARCH_PATHS": "/Applications/Xcode.app/Contents/Developer/Library/Frameworks"
  },
  "testspecs": [
    {
      "name": "UnitTests",
      "test_type": "unit",
      "source_files": "Tests/**"
    }
  ],
  "swift_version": "5.1"
}
